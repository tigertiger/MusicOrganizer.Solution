<!DOCTYPE html>
<html>
<head>
  @{
  Layout = "_Layout";
}
  @using MusicOrganizer.Models;
</head>
<body>

  <h4>Your Genre Lists</h4>

  @if (@Model.Count == 0)
  {
    <h3>None, yet...</h3>

    <a href="/genres/new"><h4>Add One</h4></a>
  } 

  @foreach (Genre genre in Model)
  {
    <h3><a href='/genres/@genre.Id'>@genre.Descrip</a></h3>
  }
</body>
</html>