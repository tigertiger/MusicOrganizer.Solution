<!DOCTYPE html>
<html>
<head>
  @{
  Layout = "_Layout";
}
  @using MusicOrganizer.Models;
</head>
<body>
  <div class="container">
    <h4>Your Genre Lists</h4>

    @if (@Model.Count == 0)
    {
      <h5>None, yet...</h5>

      <a href="/genres/new"><h4>Add One</h4></a>
    } 

    @foreach (Genre genre in Model)
    {
      <h5><a href='/genres/@genre.Id'>@genre.Descrip</a></h5>
    }
  </div>
</body>
</html>